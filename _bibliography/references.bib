@inproceedings{teixeira:2019criacao,
  title = {{Cria\c{c}\~ao de um reposit\'orio tem\'atico no contexto das humanidades digitais para gest\~ao da informa\c{c}\~ao}},
  booktitle = {{IX Confer\^encia Internacional sobre Bibliotecas e Reposit\'orios Digitais da Am\'erica Latina BIREDIAL \textendash{} ISTEC Anais das sess\~oes tem\'aticas (S\~ao Paulo, 2019)}},
  author = {Teixeira Cavalcanti, Marcia and Medeiros Pimenta, Ricardo and Cardoso Gomes, Josir},
  year = {2019},
  month = jul,
  url = {http://sedici.unlp.edu.ar/handle/10915/86544},
  urldate = {2021-06-12},
  abstract = {Em 2017 foi fundado o Laborat\'orio em Rede de Humanidades Digitais (Larhud), fruto do grupo de pesquisa que se dedica aos estudos cr\'iticos e \`as pr\'aticas em Humanidades Digitais. Sua cria\c{c}\~ao teve como objetivo construir um canal estruturado de colabora\c{c}\~ao entre os pesquisadores internos e externos com foco no desenvolvimento e na reflex\~ao sobre o pensar em solu\c{c}\~oes metodol\'ogicas para a pesquisa nas humanidades. Um caminho adotado pelo Larhud \'e criar um reposit\'orio digital tem\'atico, para reunir o conjunto da produ\c{c}\~ao cient\'ifica e acad\^emica dos integrantes do GP, contribuindo para ampliar a visibilidade de sua produ\c{c}\~ao e preservar sua mem\'oria. Um reposit\'orio pode ser definido como um local de armazenamento, de guarda e arquivamento de objetos digitais. Quanto ao seu prop\'osito, se classifica como institucional \textendash{} um espa\c{c}o onde se armazenam os documentos administrativos, acad\^emicos e cient\'ificos de uma institui\c{c}\~ao \textendash{} ou tem\'atico \textendash{} guarda objetos relacionados a uma determinada \'area sem limites institucionais \textendash{} cada um com caracter\'isticas e estrutura pr\'oprias. O artigo prop\~oe apresentar um estudo descritivo, detalhando o passo a passo relacionado\`a estrutura\c{c}\~ao das etapas de navega\c{c}\~ao, organiza\c{c}\~ao e representa\c{c}\~ao da informa\c{c}\~ao no contexto do reposit\'orio pela perspectiva da arquitetura da informa\c{c}\~ao. A etapa inicial foi identificar seus usu\'arios: administradores, pesquisadores e poss\'iveis visitantes. A etapa atual consiste na organiza\c{c}\~ao e representa\c{c}\~ao da informa\c{c}\~ao a partir da defini\c{c}\~ao das \'areas tem\'aticas, comunidades e cole\c{c}\~oes que ir\~ao compor estruturalmente o reposit\'orio, bem como da sele\c{c}\~ao do tipo de metadados que ser\~ao utilizados.},
  copyright = {http://creativecommons.org/licenses/by-nc-sa/4.0/},
  langid = {brazilian}
}

