  title = {{Aplica\c{c}\~ao da ferramenta \textsc{DSM} \textemdash{} Design Structure Matrix ao planejamento do processo de projeto de edifica\c{c}\~oes}},
  author = {Gualberto, Ana Cristina Ferrari},
  year = {2012},
