  address = {{S\~ao Paulo}},
  doi = {10.11606/D.3.2012.tde-04062013-143901},
  abstract = {O presente trabalho apresenta uma an\'alise da aplica\c{c}\~ao da ferramenta DSM Design Structure Matrix ao planejamento do processo de projeto de edifica\c{c}\~oes. O objetivo principal desta pesquisa \'e a an\'alise dos Manuais de Escopo de Projetos e Servi\c{c}os para a Ind\'ustria Imobili\'aria do ponto de vista do planejamento do processo de projeto, utilizando a ferramenta DSM. Para desenvolvimento da pesquisa primeiramente foi feita uma revis\~ao bibliogr\'afica, onde selecionou-se a metodologia ADePT para o desenvolvimento do planejamento do processo de projeto. Em seguida foram aplicadas as duas primeiras etapas da metodologia ADePT defini\c{c}\~ao do processo e otimiza\c{c}\~ao do processo, que forneceram material para uma an\'alise da aplica\c{c}\~ao da ferramenta DSM ao planejamento do processo de projeto e uma an\'alise cr\'itica sobre os Manuais de Escopo de Projetos e Servi\c{c}os para a Ind\'ustria Imobili\'aria. O cumprimento das duas primeiras etapas da metodologia ADePT forneceu material com informa\c{c}\~oes que permitiram a observa\c{c}\~ao de algumas incoer\^encias e permitiu a apresenta\c{c}\~ao de cr\'iticas e sugest\~oes de altera\c{c}\~oes em seu conte\'udo, como proposta de melhoria \`a ferramenta-guia que estes se prop\~oem a ser. Por fim, a partir da valida\c{c}\~ao da DSM como ferramenta para o desenvolvimento do planejamento do processo de projeto e com base nas altera\c{c}\~oes sugeridas para os manuais, foi proposto um novo modelo de processo de projeto.},
  langid = {brazilian},
  school = {Universidade de S\~ao Paulo, Escola Polit\'ecnica}
}

